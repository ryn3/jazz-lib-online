      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Trombone") || personnelKVObj[index].includes("trombone"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
    .col-md-12
      p
      a(style="font-size: 18px" id="other") Other:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (!(personnelKVObj[index].includes("Drums") || personnelKVObj[index].includes("drums") || personnelKVObj[index].includes("Trombone") || personnelKVObj[index].includes("trombone") || personnelKVObj[index].includes("Trumpet") || personnelKVObj[index].includes("trumpet") || personnelKVObj[index].includes("Cornet") || personnelKVObj[index].includes("cornet") || personnelKVObj[index].includes("Saxophone") || personnelKVObj[index].includes("saxophone") || personnelKVObj[index].includes("Bass") || personnelKVObj[index].includes("bass") || personnelKVObj[index].includes("Guitar") || personnelKVObj[index].includes("guitar") || personnelKVObj[index].includes("Piano") || personnelKVObj[index].includes("piano") || personnelKVObj[index].includes("Vibraphone") || personnelKVObj[index].includes("Organ")))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
    
      p
      form(method="POST", action="/albums")
        input(class="input" type="text" name="artists" size="90")
        input(hidden type="text" name="years" value=yearsQuery)
        input(hidden type="text" name="countries" value=countriesQuery)
        input(hidden type="text" name="labels" value=labelsQuery)
        input(class="btn-default" type="submit" value="Next") 
      button(class="btn-default" id="clear-me" style="cursor: pointer") Clear