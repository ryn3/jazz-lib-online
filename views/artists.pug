            if (personnelKVObj[index].includes("Piano") || personnelKVObj[index].includes("piano"))
                a(id="group") 
                    text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px" id=index)= index + ": " 
                    text(hidden)= personnelKVObj[index].substring(0,45)+"..."
