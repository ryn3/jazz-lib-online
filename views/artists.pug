      p
      a(style="font-size: 18px" id="drums") Drums:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Drums") || personnelKVObj[index].includes("drums"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "

    .col-md-6
      h4 Horns  
      p
      a(style="font-size: 18px" id="alto-saxophone") Alto Saxophone:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Alto Saxophone"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
      
      p
      a(style="font-size: 18px" id="tenor-saxophone") Tenor Saxophone:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Tenor Saxophone"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
