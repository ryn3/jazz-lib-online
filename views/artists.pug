      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Organ"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
      
      p
      a(style="font-size: 18px" id="guitar") Guitar:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Guitar") || personnelKVObj[index].includes("guitar"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
      
      p
      a(style="font-size: 18px" id="bass") Bass:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Bass") || personnelKVObj[index].includes("bass"))
