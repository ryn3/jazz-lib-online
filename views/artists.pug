extends layout



block content

  hr
  a(style="color: #3644FF" class="years-input") years 
  a(hidden)= yearsQuery
  a  >
  a(style="color: #3644FF" class="years-input")  countries
  a(hidden)= countriesQuery
  a  >
  a(style="color: #3644FF" class="years-input")  labels
  a(hidden)= labelsQuery
  a  >
  a(style="color: #3644FF")  personnel
  a  > album
  hr
  p.lead 
  .row
    .col-md-12
      abbr(class='hello') hello
      h4 Select personnel:
      a(href="#keys") piano
      a , 
      a(href="#vibes") vibes
      a , 
      a(href="#organ") organ
      a , 
      a(href="#guitar") guitar
      a , 
      a(href="#bass") bass
      a , 
      a(href="#drums") drums
      a , 
      a(href="#alto-saxophone") alto saxophone
      a , 
      a(href="#tenor-saxophone") tenor saxophone
      a , 
      a(href="#trumpet") trumpet
      a , 
      a(href="#trombone") trombone
      a , 
      a(href="#other") other


      hr
    .col-md-6
      h4 Rhythm
      p
      a(style="font-size: 18px" id="keys") Piano:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Piano") || personnelKVObj[index].includes("piano"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,45)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "
      p
      a(style="font-size: 18px" id="vibes") Vibes:
      a(class="all-persons" style="cursor: pointer")  select all
      p
          each index in personnelObj
            if (personnelKVObj[index].includes("Vibraphone"))
                text(class="choose tag" style="color: #3644FF; cursor: pointer; font-size: 14px")= index + ": " 
                text(hidden)= personnelKVObj[index].substring(0,32)+"..."
                text(style="color: #777777; font-size: 14px")= personnelObj2[index] + ", "

      p
      a(style="font-size: 18px" id="organ") Organ:
