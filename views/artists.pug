extends layout



block content

  hr
  a(style="color: #3644FF" class="years-input") years 
  a(hidden)= yearsQuery
  a  >
  a(style="color: #3644FF" class="years-input")  countries
  a(hidden)= countriesQuery
  a  >
  a(style="color: #3644FF" class="years-input")  personnel
  a(hidden)= labelsQuery
  a  >
  a  labels > album
  hr
  p.lead 
  .row
    .col-md-12
        .row
            .container.text-center
                h4.float-left Jump to personnel:
                a.float-right Sort by: 
                    button(class="btn-default alphabetical" style="cursor: pointer") alphabetical
                    button(class="btn-default count-persons" style="color: #3644FF; cursor: pointer")  count
        .row
            .container
                a(href="#keys") piano
                a , 
                a(href="#vibes") vibes
                a , 
                a(href="#organ") organ
                a , 
                a(href="#guitar") guitar
                a , 
                a(href="#bass") bass
                a , 
                a(href="#drums") drums
                a , 
                a(href="#alto-saxophone") alto saxophone
                a , 
                a(href="#tenor-saxophone") tenor saxophone
                a , 
                a(href="#trumpet") trumpet
                a , 
                a(href="#trombone") trombone
                a , 
                a(href="#other") other
